#!/sbin/sh
# KernelSU Module Installer

OUTFD=$2
ZIPFILE=$3

# Extract files
unzip -o "$ZIPFILE" -d "$MODPATH" >/dev/null 2>&1

# Set permissions
set_perm_recursive $MODPATH 0 0 0755 0644
chmod 755 $MODPATH/post-fs-data.sh
chmod 755 $MODPATH/service.sh

ui_print "- Remove Evolution X Hooks Module installed"
ui_print "- Reboot required to take effect"
